// While loop test
// "Complex way" of a summation of two integers

Program: Main()

class Main is
    this() is

        var a : Integer
        var b : Integer
        a.scan()
        b.scan()

        while a.Greater(0) loop
            b := b.Plus(1)
            a := a.Minus(1)
            a.print()
        end

        b.print()

    end

end