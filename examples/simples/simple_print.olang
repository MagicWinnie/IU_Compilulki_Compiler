// While loop test
// "Complex way" of a summation of two integers

Program: Main()

class Base is
    var b : Integer(123)
   method test(a : Integer) is

        b.print()
   end

   this() is
   b := Integer(2222)
   end

   method show() is
    b.print()

   end
end


class Main is
    this() is
        var d : Base()
        var t : 52

        d.test(t)
        d.show()
    end
end