// While loop test
// "Complex way" of a summation of two integers

Program: Main()

class Main is
    this() is

        var a : Integer(3)
        var b : Integer(2)

        while a.Greater(0) loop
            b := b.Plus(1)
            a := a.Minus(1)
            b.print()
        end

        b.print()

    end

end