// Array test
// Creating and printing the array of cubes of integers from 0 to 4

Program: Main

class Main is
    this() is

        var a : IntArray(10)

        var i1 : Integer(0)

        var n : Integer(10)

        while i1.Less(n) loop
            a.set(i1, i1)
            i1 := i1.Plus(1)
        end

        var i2 : Integer(9)
        while i2.GreaterEqual(0) loop
            a.get(i2).print()
            i2 := i2.Minus(1)
        end

        var l : Integer()
        l := a.Length()
        l.print()


        var b : BoolArray(10)

        var k : Integer()
        k := b.Length()
        k.print()



    end

end