// Array test
// Creating and printing the array of cubes of integers from 0 to 4

Program: Main

class Main is
    this() is

        var a : Array[Integer](5)

        var i1 : Integer(0)
        while i1.LessEqual(4) loop
            a.set(i1, i1.Mult(i1).Mult(i1))
        end

        var i2 : Integer(4)
        while i2.GreaterEqual(0) loop
            a.get(i2).print()
        end

    end

end