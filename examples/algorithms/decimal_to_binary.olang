// Перевод числа из десятичной системы счисления в двоичную

Program: Main()

class Main is
    this() is
        var n : 78

        var b : IntArray(10)
        var i : 0
        while n.Greater(0) loop
            b.set(i, n.Rem(2))
            i := i.Plus(1)
            n := n.Div(2)
        end


        i := i.Minus(1)

        while i.Greater(0) loop
            b.get(i).print()
            i := i.Minus(1)
        end

        b.get(0).print()
    end
end